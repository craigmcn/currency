{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.formatOnSave": false,
    "[html]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
      "editor.formatOnSave": true,
    },
    "[typescript]": {
      "editor.formatOnSave": true,
    },
    "[scss]": {
      "editor.formatOnSave": true,
    },
    "editor.detectIndentation": false,
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "eslint.options": {
        "ignorePattern": "webpack.config.js",
    },
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "beautify.language": {
      "css": ["css", "scss"],
    },
    "cSpell.words": [
        "craigmcn",
        "favicon",
        "fontawesome",
        "fortawesome",
        "ipdata",
        "ipdataco",
        "msapplication",
        "noscript",
        "stylesheet"
    ],
    "cSpell.ignorePaths": [
        ".eslintrc",
        "package.json",
        "package-lock.json",
        "webpack.config.js",
    ],
  }
  